    <property name="BlackBeans" column="BlackBeans" />
    <property name="PintoBeans" column="PintoBeans" />
    <property name="Chicken" column="Chicken" />
    <property name="Beef" column="Beef" />
    <property name="Hummus" column="Hummus" />
    <property name="SalsaPico" column="SalsaPico" />
    <property name="SalsaVerde" column="SalsaVerde" />
    <property name="SalsaSpecial" column="SalsaSpecial" />
    <property name="Guacamole" column="Guacamole" />
    <property name="Lettuce" column="Lettuce" />
    <property name="Jalapenos" column="Jalapenos" />
    <property name="Tomatoes" column="Tomatoes" />
    <property name="Cucumber" column="Cucumber" />
    <property name="Onion" column="Onion" />
    <property name="Price" column="Price" />
  </class>
</hibernate-mapping>